{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "flask-app",
        "image": "<your-dockerhub-username>/flask-app:latest",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 5000,
            "containerPort": 5000
          }
        ]
      },
      {
        "name": "nodejs-app",
        "image": "<your-dockerhub-username>/nodejs-app:latest",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 3000,
            "containerPort": 3000
          }
        ]
      }
    ]
  }
  